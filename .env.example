# TidyGuru Environment Variables

# App Configuration
VITE_APP_NAME=TidyGuru
VITE_APP_VERSION=1.0.0

# File Upload Limits
VITE_MAX_FILE_SIZE_MB=10
VITE_MAX_ROWS=50000

# Feature Flags
VITE_ENABLE_PDF_EXPORT=true
VITE_ENABLE_CSV_EXPORT=true
VITE_ENABLE_KEYBOARD_SHORTCUTS=true

# Analytics (optional)
# VITE_ANALYTICS_ID=

# API Endpoints (for future use)
# VITE_API_URL=
